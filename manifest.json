{
  "name": "ZoomTabCloser",
  "description": "Chrome extension to automatically close google chrome tabs used to launch zoom.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["alarms", "storage", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/zoomninja16.png",
      "32": "images/zoomninja32.png",
      "48": "images/zoomninja48.png",
      "128": "images/zoomninja128.png"
    }
  },
  "icons": {
    "16": "images/zoomninja16.png",
    "32": "images/zoomninja32.png",
    "48": "images/zoomninja48.png",
    "128": "images/zoomninja128.png"
  },
  "options_page": "options.html"
}
